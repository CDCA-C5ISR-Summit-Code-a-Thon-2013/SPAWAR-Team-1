<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tigerstripestech.codeathon"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tigerstripestech.codeathon.MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tigerstripestech.codeathon.RecordMeal"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_record_meal"
            android:parentActivityName="com.tigerstripestech.codeathon.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tigerstripestech.codeathon.MainActivity" />
        </activity>
        <activity
            android:name="com.tigerstripestech.codeathon.Profile"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_profile"
            android:parentActivityName="com.tigerstripestech.codeathon.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tigerstripestech.codeathon.MainActivity" />
        </activity>
        <activity
            android:name="com.tigerstripestech.codeathon.CalendarActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_calendar_activity"
            android:parentActivityName="com.tigerstripestech.codeathon.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tigerstripestech.codeathon.MainActivity" />
        </activity>
       	<activity
            android:name="com.tigerstripestech.codeathon.IntakeSelect"
            android:screenOrientation="portrait"
            android:label="Intake Overview"
            android:parentActivityName="com.tigerstripestech.codeathon.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tigerstripestech.codeathon.MainActivity" />
        </activity>
    </application>

</manifest>
